+import androidx.appcompat.app.AppCompatActivity;
+import androidx.constraintlayout.widget.ConstraintLayout;
+
+import java.util.Locale;
+
+public class AnaSayfa extends AppCompatActivity {
+
+    private DatabaseHelper dbHelper;
+
+    @SuppressLint("MissingInflatedId")
+    @Override
+    protected void onCreate(Bundle savedInstanceState) {
+        super.onCreate(savedInstanceState);
+        setContentView(R.layout.activity_ana_sayfa);
+
+
+
+        dbHelper = new DatabaseHelper(this);
+        SQLiteDatabase db = dbHelper.getWritableDatabase();
+    }
+
+
+
+    public void gecis(View v) {
+        Intent az = new Intent(AnaSayfa.this, Calculation.class);
+        startActivity(az);
+    }
+
+    public void history(View v) {
+        Intent ax = new Intent(AnaSayfa.this, History.class);
+        startActivity(ax);
+    }
+
+    public void whatco(View v) {
+        Intent cv = new Intent(AnaSayfa.this, WhatCarbonFootPrint.class);
+        startActivity(cv);
+    }
+
+
+}
